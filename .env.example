# Example environment variables for LokiAI Portfolio Rebalancer
# Copy to .env and adjust values

# MongoDB
MONGO_URI=mongodb://localhost:27017
MONGO_DB_NAME=loki_agents

# Wallet and keys (never commit real keys)
WALLET_ADDRESS=0xYourWallet
PRIVATE_KEY=your_private_key_hex

# Alerts (if you prefer env over config.json)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
DISCORD_WEBHOOK_URL=
EMAIL_SMTP=smtp.gmail.com
EMAIL_USER=
EMAIL_PASS=
EMAIL_TO=
